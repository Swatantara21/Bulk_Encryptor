$pipe in_data  : $uint<64> $depth 2
$pipe out_data : $uint<64> $depth 2
 
$module [MAIN_daemon] $in() $out () $is
{	
	
	$branchblock[MAIN]  {
	
	$dopipeline  $depth 2 $buffering 2 
	$merge $entry $loopback $endmerge
	
		Ina := in_data
		Inb := in_data
			
		$volatile $split (Ina 32 32)  (X0 X1)
		$volatile $split (Inb 32 32)  (X2 X3)	

		$volatile Y0 := X0
		$volatile Y1 := (X1 + 1)
		$volatile Y2 := (X2 + 2)
		$volatile Y3 := (X3 + 3)
		
		$volatile Za := ($concat Y0 Y1)
		$volatile Zb := ($concat Y2 Y3)

		out_data := Za
		out_data := Zb
		
               
	$while 1
	
    }
}

