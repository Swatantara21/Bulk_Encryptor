digraph control_path {
  e_0: entry_node : n ;
  e_1 [shape = diamond];
  e_2 [shape = diamond];
  e_3 [shape = diamond];
  e_4 [shape = diamond];
  e_5 [shape = triangle];
  e_6 [shape = diamond];
  e_7 [shape = diamond];
  e_8: exit_node : s ;
  e_0 -> e_1;
  e_1 -> e_2;
  e_2 -> e_3;
  e_3 -> e_4;
  e_4 -> e_5;
  e_4 -> e_6;
  e_6 -> e_7;
  e_7 -> e_8;
}
